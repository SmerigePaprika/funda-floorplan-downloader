{
  "manifest_version": 3,
  "name": "Funda Floorplan Downloader",
  "version": "1.0",
  "description": "Detects and downloads FML floorplans from funda.nl property listings.",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://www.funda.nl/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  },
  "icons": {
    "128": "icon128.png"
  }
}
